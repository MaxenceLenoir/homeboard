<h1>Mes voisins</h1>

<% @users.each do |user| %>
    <p><%= user.first_name %> <%= user.last_name %>
    <%if current_user == user %>
        <%= link_to "My profil", profile_path(user) %>
    <% else %>
        <%= link_to "Read more", profile_path(user) %>
    <% end %>
    
    
    </p>
     
        
<%end %>
